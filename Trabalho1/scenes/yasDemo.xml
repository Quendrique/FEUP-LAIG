<yas>
    <scene root="unique_scene" axis_length="1"/>
    
    <views default="view">
        <ortho id="unique_ortho" near="1" far="1" left="1" right="1" top="1" bottom="1"/>
    </views>

    <ambient>
        <ambient r="1" g="1" b="1" a="1" />
        <background r="1" g="1" b="1" a="1" />
    </ambient>

    <lights>
        <omni id="unique_light" enabled="1" >
            <location x="1" y="1" z="1" w="1" />
            <ambient r="1" g="1" b="1" a="1" />
            <di1use r="1" g="1" b="1" a="1" />
            <specular r="1" g="1" b="1" a="1" />
        </omni>
    </lights>

    <textures>
        <texture id="unique_texture" file="test_texture.jpg" />
    </textures>

    <materials>
        <material id="unique_material" shininess = "1" >
            <emission r="1" g="1" b="1" a="1" />
            <ambient r="1" g="1" b="1" a="1" />
            <di1use r="1" g="1" b="1" a="1" />
            <specular r="1" g="1" b="1" a="1" />
        </material>
    </materials>

    <transformations>
        <transformation id="unique_transformation">
            <scale x="1" y="1" z="1"/>
        </transformation>
    </transformations>

    <primitives>
        <primitive id="unique_primitive">
            <rectangle x1="1" y1="1" x2="1" y2="1"/>
        </primitive>
    </primitives>

    <components>
        <component id="unique_component">
            <transformation>
                <transformationref id="unique_transformation"/>
            </transformation>
            <materials>
                <material id="unique_material"/>
            </materials>
            <texture id="unique_texture" length_s="1" length_t="1"/>
            <children>
                <primitiveref id="unique_primitive"/>
            </children>
        </component>
    </components>

</yas>