<yas>
    <scene root="root" axis_length="5"/>
    
    <views default="overview"> 
        <perspective id="overview" near="0.1" far="30" angle="45">
            <from x="10" y="10" z="10" />
            <to x="0" y="0" z="0" />
        </perspective>

        <perspective id="saturn" near="0.1" far="30" angle="45">
            <from x="16" y="14" z="14" />
            <to x="0" y="0" z="0" />
        </perspective>

        <perspective id="behind_desk" near="0.1" far="30" angle="45">
            <from x="3" y="3" z="0.9" />
            <to x="3" y="3" z="7" />
        </perspective>

        <perspective id="room_corner" near="0.1" far="30" angle="45">
            <from x="0.1" y="0.1" z="0.1" />
            <to x="7" y="7" z="7" />
        </perspective>

        <ortho id="room" near="0.1" far="20" left="-7" right="7" top="3.5" bottom="-3.5">
            <from x="3.5" y="3.5" z="7" />
            <to x="3.5" y="3.5" z="0" />
        </ortho>

        <ortho id="painting" near="0.1" far="20" left="-3.5" right="3.5" top="1.75" bottom="-1.75">
            <from x="1" y="4" z="3.5" />
            <to x="0" y="4" z="3.5" />
        </ortho>
    </views>

    <ambient>
        <ambient r="0.6" g="0.6" b="0.6" a="1" />
        <background r="0.07" g="0.07" b="0.07" a="1" />
    </ambient>

    <lights>
        <omni id="lamp" enabled="1">
            <location x="3" y="5.6" z="2.7" w="1"/>
            <ambient r="0" g="0" b="0" a="1"/>
            <diffuse r="0.6" g="0.6" b="0.6" a="1"/>
            <specular r="0.6" g="0.6" b="0.6" a="1"/>
        </omni>

        <omni id="corner" enabled="0">
            <location x="0.3" y="6.7" z="0.3" w="1"/>
            <ambient r="0" g="0" b="0" a="1"/>
            <diffuse r="1" g="1" b="1" a="1"/>
            <specular r="1" g="1" b="1" a="1"/>
        </omni>

        <spot angle="1" id="painting" enabled="0" exponent="6">
   	        <location x="0.6" y="6.5" z="3.5" w="1"/>
	        <target x="0" y="4.5" z="3.5"/>
     	    <ambient r="0" g="0" b="0" a="1"/>
	        <diffuse r="1" g="1" b="1" a="1"/>
     	    <specular r="1" g="1" b="1" a="1"/>
        </spot>

        <omni id="red_led" enabled="0">
            <location x="2.7" y="2.5" z="4.3" w="1"/>
            <ambient r="0" g="0" b="0" a="1"/>
            <diffuse r="0.4" g="0" b="0" a="1"/>
            <specular r="0.4" g="0" b="0" a="1"/>
        </omni>

        <omni id="green_led" enabled="0">
            <location x="3" y="2.5" z="4.3" w="1"/>
            <ambient r="0" g="0" b="0" a="1"/>
            <diffuse r="0" g="0.4" b="0" a="1"/>
            <specular r="0" g="0.4" b="0" a="1"/>
        </omni>

        <omni id="blue_led" enabled="0">
            <location x="3.3" y="2.5" z="4.3" w="1"/>
            <ambient r="0" g="0" b="0" a="1"/>
            <diffuse r="0" g="0" b="0.4" a="1"/>
            <specular r="0" g="0" b="0.4" a="1"/>
        </omni>
    </lights>

    <textures>
        <texture id="windows" file="./scenes/images/windows.jpg"/>
        <texture id="black_plastic" file="./scenes/images/black_plastic.jpg"/>
        <texture id="gray_wood" file="./scenes/images/gray_wood.jpg"/>
        <texture id="velvet" file="./scenes/images/velvet.jpg"/>
        <texture id="black_wood" file="./scenes/images/black_wood.jpg"/>
        <texture id="gray_wash" file="./scenes/images/gray_wash.png"/>
        <texture id="gray_stone" file="./scenes/images/gray_stone.jpg"/>
        <texture id="stainless_steel" file="./scenes/images/stainless_steel.jpg"/>
        <texture id="trash" file="./scenes/images/trash.jpg"/>
        <texture id="saturn" file="./scenes/images/saturn.jpg"/>
        <texture id="saturn_ring" file="./scenes/images/saturn_ring.jpg"/>
        <texture id="linen" file="./scenes/images/linen.jpg"/>
        <texture id="dolomite" file="./scenes/images/dolomite.jpg"/>
        <texture id="gold" file="./scenes/images/gold.jpeg"/>
        <texture id="vidal" file="./scenes/images/vidal.jpg"/>
        <texture id="picture" file="./scenes/images/picture.jpg"/>
    </textures>

    <materials>
        <material id="default" shininess = "30" >
            <emission r="0" g="0" b="0" a="1" /> 
            <ambient  r="0.1" g="0.1" b="0.1" a="1" /> 
            <diffuse  r="0.80" g="0.80" b="0.80" a="1" /> 
            <specular  r="0.80" g="0.80" b="0.80" a="1" /> 
        </material>

        <material id="vase" shininess="20">
