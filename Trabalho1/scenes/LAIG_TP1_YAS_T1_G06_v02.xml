            <diffuse a="1" b="0.045" g="0.15" r="0.300"/>
            <specular a="1" b="0.015" g="0.042" r="0.1"/>
        </material>

        <material id="velvet" shininess="30" >
            <emission r="0" g="0" b="0" a="1" />
            <ambient  r="0.1" g="0.1" b="0.1" a="1" />  
            <diffuse r="0.80" g="0.30" b="0.30" a="1" />
            <specular r="0.80" g="0.30" b="0.30" a="1" />
        </material>

        <material id="dark" shininess = "20" >
            <emission r="0" g="0" b="0" a="1" /> 
            <ambient  r="0.1" g="0.1" b="0.1" a="1" />   
            <diffuse  r="0.40" g="0.40" b="0.40" a="1" /> 
            <specular  r="0.40" g="0.40" b="0.40" a="1" /> 
        </material>

        <material id="source" shininess="100" >
            <emission r="1" g="1" b="1" a="1" />
            <ambient  r="0.1" g="0.1" b="0.1" a="1" />  
            <diffuse r="1" g="1" b="1" a="1" />
            <specular r="1" g="1" b="1" a="1" />
        </material>

        <material id="metal" shininess="80" >
            <emission r="0" g="0" b="0" a="1" />
            <ambient  r="0.1" g="0.1" b="0.1" a="1" /> 
            <diffuse r="0.75" g="0.75" b="0.75" a="1" />
            <specular r="0.75" g="0.75" b="0.75" a="1" />
        </material>

        <material id="gold" shininess="40" >
            <emission r="0" g="0" b="0" a="1" />
            <ambient  r="0.1" g="0.1" b="0.1" a="1" /> 
            <diffuse r="0.9" g="0.6" b="0.1" a="1" />
            <specular r="0.9" g="0.6" b="0.1" a="1" />
        </material>

        <material id="cream" shininess="30" >
            <emission r="0" g="0" b="0" a="1" />
            <ambient  r="0.1" g="0.1" b="0.1" a="1" /> 
            <diffuse r="0.6" g="0.5" b="0.3" a="1" />
            <specular r="0.6" g="0.5" b="0.1" a="1" />
