                <componentref id="side_wall"/>
                <componentref id="back_wall"/>
                <componentref id="ground"/>
                <componentref id="ceiling"/>
                <componentref id="tv"/>
                <componentref id="table"/>
                <componentref id="saturn"/>
                <componentref id="ceiling_lamp"/>
